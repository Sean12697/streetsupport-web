{{> no-js}}

<div id="js-category-result-output" class="template"></div>

<div id="spin"></div>

<script id="js-category-no-results-result-tpl" type="text/template">
  {{> find-help/breadcrumb }}

  <div class="block">
    <div class="container">
      <div class="block__content">
        {{> find-help/header }}
        {{> proximity-search/hogan }}
      </div>
    </div>
  </div>

  {{> find-help/view-type-tabs mapIsSelected="true" }}

  <div class="block block--cards">
    <div class="container">
      <div class="block__content">
        {{> no-services geoLocationUnavailable="geoLocationUnavailable" }}
      </div>
    </div>
  </div>
</script>

<script id="js-category-result-tpl" type="text/template">
  {{> find-help/breadcrumb }}

  <div class="block">
    <div class="container">
      <div class="block__content">
        {{> find-help/header }}
        {{> proximity-search/hogan }}
      </div>
    </div>
  </div>

  {{> find-help/view-type-tabs mapIsSelected="true" }}

  <div class="block block--cards">
    <div class="container">
      <div class="block__content">
        <div class="js-map services-map"></div>
      </div>

      <p class="text-center">Please note, the information here is provided and maintained by the organisations. If you see anything that is incorrect or incomplete, please <a href="/about/contact/">get in touch</a>.</p>

      {{> social-share}}

    </div>
  </div>
</div>
</script>
